    "terminal.integrated.fontFamily": "Inconsolata-g",
    "terminal.integrated.macOptionIsMeta": true,
    "workbench.startupEditor": "newUntitledFile",
    "python.dataScience.sendSelectionToInteractiveWindow": true,
    "vs-kubernetes": {
        "vs-kubernetes.helm-path": "/Users/kleimkuhler/.vs-kubernetes/tools/helm/darwin-amd64/helm",
        "vs-kubernetes.minikube-path": "/Users/kleimkuhler/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
        "vs-kubernetes.draft-path": "/Users/kleimkuhler/.vs-kubernetes/tools/draft/darwin-amd64/draft"
    },
    "workbench.colorTheme": "Material Theme Palenight",
    "editor.formatOnSave": true,
